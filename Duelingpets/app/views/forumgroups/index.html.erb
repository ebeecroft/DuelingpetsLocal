<% provide(:h1, "Forumgroup List") %>
<% @forumgroups.each do |forumgroup| %>
   <br>
   <p>Group: <%= forumgroup.name %></p>
   <p>Created on: <%= distance_of_time_in_words_to_now forumgroup.created_on %> ago on (<%= forumgroup.created_on.strftime("%B-%d-%Y") %>)</p>
<% end %>
<br>
<p class="navlinks"><%= link_to 'Back', root_path %></p>
